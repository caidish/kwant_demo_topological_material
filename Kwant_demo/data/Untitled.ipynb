   "metadata": {},
   "outputs": [],
   "source": [
    "gate_list = np.linspace(-2,2,100)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "for m,gate in enumerate(gate_list):\n",
    "    a = sio.loadmat(str(gate)+\".mat\")\n",
    "    sio.savemat('data'+str(m)+\".mat\",a)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
